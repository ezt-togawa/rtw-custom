<odoo>
  <data>
    <record id="action_picking_itoshima_filter" model="ir.actions.act_window">
      <field name="name">Stock Picking Itoshima Filter</field>
      <field name="res_model">stock.picking</field>
      <field name="type">ir.actions.act_window</field>
      <field name="view_mode">tree,kanban,form,calendar</field>
      <field name="domain"></field>
      <field name="context">{'search_default_waiting':1,'search_default_available':1,'search_default_picking_type_id':[active_id]}</field>
      <field name="search_view_id" ref="stock.view_picking_internal_search"/>
      <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
              No transfer found. Let's create one!
          </p><p>
              Transfers allow you to move products from one location to another.
          </p>
      </field>
  </record>
  </data>
</odoo>
