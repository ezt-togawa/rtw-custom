<odoo>
    <data>
       <record id="stock_move_line_list_container" model="ir.ui.view">
           <field name="name">move_line_list_container</field>
           <field name="model">stock.move.line</field>
           <field name="inherit_id" ref="rtw_stock_move_line.stock_move_line_list_itoshima"/>
           <field name="arch" type="xml">
               <xpath expr="//field[@name='result_package_id']" position="after">
                   <field name="container_id" optional="show"/>
               </xpath>
           </field>
       </record>
    </data>
</odoo>
