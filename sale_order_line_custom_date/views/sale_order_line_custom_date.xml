<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_order_line_cusom_datetree" model="ir.ui.view">
        <field name="name">sale.order.custom_date_tree</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="/form/sheet/notebook/page/field[@name='order_line']/tree/field[@name='price_subtotal']" position="after">
                <field name="depo_date" optional="hide" />
                <field name="shiratani_date" optional="hide" />
                <field name="p_type" optional="hide" />
            </xpath>
        </field>

    </record>
</odoo>
